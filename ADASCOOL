//Problem - https://www.codechef.com/problems/ADASCOOL

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        int i;
        for(i=0;i<t;i++)
        {
            int n=sc.nextInt();
            int m=sc.nextInt();
            if(n%2==0 || m%2==0)
            System.out.println("YES");
            else
            System.out.println("NO");
        }
    }
}
