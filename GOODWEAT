// https://www.codechef.com/problems/GOODWEAT

#include <iostream>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--) {
	    int a[7];
	    int a1=0, a0=0;
	    for(int i =0; i < 7; i++) {
	        cin >> a[i];
	        if(a[i] == 1) {
	            a1++;
	        }
	        else{
	            a0++;
	        }
	    }
	    if(a1 > a0) {
	        cout << "YES" << endl;
	    }
	    else{
	        cout << "NO" << endl;
	    }
	    
	}
	return 0;
}
