//start 
import java.util.*;
class Pattern2
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int row, col;
		int n = sc.nextInt();
         
        for(row = 1; row<=n; row++)
        {
            for(col = n; col>=1; col--)
            {
                System.out.print(col+" ");
            }
            System.out.println();
        }
	}
}
//End 	


