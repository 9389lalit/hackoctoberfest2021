import java.util.*;
class Add
{
	public static void main(String[] args) {
			
	Scanner sc = new Scanner(System.in);
		
		int a;
		a = sc.nextInt();
     /* 	-----This Is Comment------- 
     for (int i=1; i<=a; i++) 
     */ 
       for (int i=a; i>=1; i--)  	
		
		if(i%2 !=0)
		{
			System.out.print(i);
		}


	}
}
